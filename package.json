{
  "name": "detox-puppeteer",
  "version": "4.0.1",
  "main": "build/PuppeteerDriver.js",
  "private": false,
  "scripts": {
    "build": "tsc",
    "test": "tsc && detox test --configuration simple"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "puppeteer": "^10.0.0",
    "tslib": "^2.2.0",
    "xvfb": "^0.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.4",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "detox": "next",
    "jest": "^27.3.1",
    "prettier": "^1.19.0",
    "typescript": "4.2.4"
  },
  "detox": {
    "artifacts": {
      "plugins": {
        "uiHierarchy": "enabled"
      }
    },
    "behavior": {
      "init": {
        "launchApp": false
      }
    },
    "devices": {
      "puppeteer": {
        "type": "./build/PuppeteerDriver.js",
        "defaultViewport": {
          "width": 375,
          "height": 712
        }
      }
    },
    "apps": {
      "simple": {
        "type": "chrome",
        "binaryPath": "http://localhost:8889/"
      }
    },
    "session": {},
    "configurations": {
      "simple": {
        "device": "puppeteer",
        "app": "simple"
      },
      "android.emu.release": {
        "device": "emulator",
        "app": "android.release"
      },
      "android.att.release": {
        "device": "android.attached",
        "app": "android.release"
      },
      "android.genymotion.release": {
        "device": "android.genycloud",
        "app": "android.release"
      }
    },
    "configurations_old": {
      "simple": {
        "binaryPath": "http://localhost:8889/",
        "type": "./build/PuppeteerDriver.js",
        "device": {
          "defaultViewport": {
            "width": 375,
            "height": 712
          }
        },
        "name": "puppeteer"
      }
    },
    "test-runner": "jest"
  }
}
