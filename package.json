{
  "name": "detox-puppeteer",
  "version": "5.0.0",
  "main": "build/PuppeteerDriver.js",
  "private": false,
  "scripts": {
    "build": "tsc",
    "test": "tsc && detox test --configuration simple",
    "test-video": "detox test --configuration simple --record-videos all"
  },
  "peerDependencies": {
    "detox": "^20.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "puppeteer": "^11.0.0",
    "puppeteer-screen-recorder": "^2.0.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.4",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "detox": "^20.0.0",
    "jest": "^27.3.1",
    "prettier": "^1.19.0",
    "typescript": "4.2.4"
  },
  "detox": {
    "testRunner": {
      "args": {
        "$0": "jest",
        "config": "e2e/jest.config.js"
      },
      "forwardEnv": true
    },
    "devices": {
      "puppeteer": {
        "type": "./build/PuppeteerDriver.js",
        "defaultViewport": {
          "width": 375,
          "height": 712
        }
      }
    },
    "apps": {
      "simple": {
        "type": "chrome",
        "binaryPath": "http://localhost:8889/"
      }
    },
    "session": {},
    "configurations": {
      "simple": {
        "device": "puppeteer",
        "app": "simple"
      },
      "android.emu.release": {
        "device": "emulator",
        "app": "android.release"
      },
      "android.att.release": {
        "device": "android.attached",
        "app": "android.release"
      },
      "android.genymotion.release": {
        "device": "android.genycloud",
        "app": "android.release"
      }
    }
  }
}
